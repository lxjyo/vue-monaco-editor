{
  "name": "vue-monaco-editor",
  "version": "0.9.13",
  "description": "monaco-editor vue3组件",
  "scripts": {
    "dev": "pnpm -C examples/esm-demo dev",
    "build": "rollup -c rollup.config.mjs",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "commit": "git cz",
    "release": "HUSKY_SKIP_HOOKS=1 standard-version"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "components/**/*.{vue,js,jsx,cjs,mjs,ts,tsx,cts,mts}": [
      "eslint --fix"
    ]
  },
  "keywords": [
    "monaco-editor",
    "vue3",
    "diff"
  ],
  "author": "lxjyo",
  "license": "ISC",
  "dependencies": {
    "monaco-components": "workspace:^",
    "monaco-editor": "^0.50.0",
    "vue": "^3.4.32"
  },
  "devDependencies": {
    "@monaco-editor/loader": "^1.4.0",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rushstack/eslint-patch": "^1.1.4",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "husky": "^9.1.3",
    "less": "^4.2.0",
    "lint-staged": "^15.2.7",
    "prettier": "^2.7.1",
    "rollup": "^4.18.1",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-esbuild": "^6.1.1",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "rollup-plugin-vue": "^6.0.0",
    "standard-version": "^9.5.0",
    "typescript": "^4.9.5"
  },
  "repository": {
    "url": "https://github.com/lxjyo/vue-monaco-editor.git"
  }
}
